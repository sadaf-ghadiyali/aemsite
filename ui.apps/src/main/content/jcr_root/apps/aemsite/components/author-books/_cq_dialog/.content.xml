<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Author Books"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <multifieldwithmap
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Multifield with Map"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <authorname
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                emptyText="Enter Author's name"
                                fieldLabel="Author Name"
                                name="./authorname"/>
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                <items jcr:primaryType="nt:unstructured">
                                    <books
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        fieldLabel="Book Name">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            name="./books"/>
                                    </books>
                                    <bookdetailswithmap
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Book Details">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./bookdetailswithmap">
                                            <items jcr:primaryType="nt:unstructured">
                                                <bookname
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Name"
                                                    fieldLabel="Book Name"
                                                    name="./bookname"/>
                                                <booksubject
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Subject"
                                                    fieldLabel="Book Subject"
                                                    name="./booksubject"/>
                                                <publishyear
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Publish year"
                                                    fieldLabel="Publish Year"
                                                    name="./publishyear"/>
                                            </items>
                                        </field>
                                    </bookdetailswithmap>
                                </items>
                            </well>
                        </items>
                    </multifieldwithmap>
                    <multifieldwithbean
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Multifield with Bean"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                <items jcr:primaryType="nt:unstructured">
                                    <bookdetailswithbean
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Book Details">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./bookdetailswithbean">
                                            <items jcr:primaryType="nt:unstructured">
                                                <bookname
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Name"
                                                    fieldLabel="Book Name"
                                                    name="./bookname"/>
                                                <booksubject
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Subject"
                                                    fieldLabel="Book Subject"
                                                    name="./booksubject"/>
                                                <publishyear
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Publish year"
                                                    fieldLabel="Publish Year"
                                                    name="./publishyear"/>
                                            </items>
                                        </field>
                                    </bookdetailswithbean>
                                </items>
                            </well>
                        </items>
                    </multifieldwithbean>
                    <nestedmultifield
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Nested Multifield"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                <items jcr:primaryType="nt:unstructured">
                                    <bookdetailswithnestedmultifield
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Book Details">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./nestedbookdetails">
                                            <items jcr:primaryType="nt:unstructured">
                                                <bookname
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Name"
                                                    fieldLabel="Book Name"
                                                    name="./bookname"/>
                                                <booksubject
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Subject"
                                                    fieldLabel="Book Subject"
                                                    name="./booksubject"/>
                                                <publishyear
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter Book Publish year"
                                                    fieldLabel="Publish Year"
                                                    name="./publishyear"/>
                                                <bookeditions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./bookeditions">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <bookedition
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="Enter Book Edition"
                                                                fieldLabel="Book Edition"
                                                                name="./bookedition"/>
                                                            <editiondate
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                                fieldLabel="Edition Date"
                                                                name="./editiondate"/>
                                                        </items>
                                                    </field>
                                                </bookeditions>
                                            </items>
                                        </field>
                                    </bookdetailswithnestedmultifield>
                                </items>
                            </well>
                        </items>
                    </nestedmultifield>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
